{
  "name": "recaura.clientapp",
  "version": "1.0.0",
  "description": "physiotherapy management software",
  "main": "index.js",
  "scripts": {
    "start": "npm run build:dev",
    "build": "npm run build:prod",
    "build:dev": "node --max-old-space-size=8192 node_modules/webpack-dev-server/bin/webpack-dev-server.js --env.ENVIRONMENT=development --config webpack.dev.js --host 0.0.0.0 --port 3000 --progress",
    "build:prod": "rimraf ./dist/bundle/* && webpack -p --env.ENVIRONMENT=production --config webpack.prod.js --progress",
    "test": "jest",
    "test:watch": "npm test -- --watch"
  },
  "author": "Jeffrey Huang",
  "license": "ISC",
  "dependencies": {
    "antd": "^4.1.0",
    "axios": "^0.19.2",
    "connected-react-router": "^6.8.0",
    "history": "^4.10.1",
    "less": "^3.11.1",
    "moment": "^2.24.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router": "^5.1.2",
    "react-router-dom": "^5.1.2",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/history": "^4.7.5",
    "@types/jest": "^25.2.1",
    "@types/react": "^16.9.27",
    "@types/react-dom": "^16.9.5",
    "@types/react-redux": "^7.1.7",
    "@types/react-router": "^5.1.5",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-mock-store": "^1.0.2",
    "assets-webpack-plugin": "^3.9.12",
    "axios-mock-adapter": "^1.18.1",
    "babel-loader": "^8.1.0",
    "case-sensitive-paths-webpack-plugin": "^2.3.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.4.2",
    "dotenv": "^8.2.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "^6.8.0",
    "fork-ts-checker-webpack-plugin": "^4.1.2",
    "html-webpack-plugin": "^4.0.4",
    "jest": "^25.4.0",
    "less-loader": "^5.0.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.13.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-mock-store": "^1.5.4",
    "rimraf": "^3.0.2",
    "sass-loader": "^8.0.2",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.3.5",
    "ts-jest": "^25.4.0",
    "ts-loader": "^6.2.2",
    "typescript": "^3.8.3",
    "warnings-to-errors-webpack-plugin": "^2.0.0",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^4.2.2",
    "write-file-webpack-plugin": "^4.5.1"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "verbose": true,
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx"
    ],
    "moduleNameMapper": {
      "\\.(css|sass|scss)$": "<rootDir>__tests__/setup/styleMock.ts",
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>__tests__/setup/fileMock.ts"
    },
    "testPathIgnorePatterns": [
      "<rootDir>/__tests__/setup/",
      "/node_modules/",
      "/dist/"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "setupFiles": [
      "<rootDir>__tests__/setup/setupEnzyme.ts"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>__tests__/setup/matchMedia.ts"
    ]
  }
}
