{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "client",
        "image": "jeffvh/recaura-client",
        "hostname": "client",
        "essential": false,
        "memory": 128
      },
      {
        "name": "api",
        "image": "jeffvh/recaura-api",
        "hostname": "api",
        "essential": false,
        "memory": 128
      },
      {
        "name": "nginx",
        "image": "jeffvh/recaura-nginx",
        "essential": true,
        "memory": 128,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 80
          }
        ],
        "links": ["client", "api"]
      }
    ]
  }